<p>bookmarks</p>
<ul>
  <% @bookmarks.each do |bookmark| %>
  <li>
    <a href="<%= bookmark.url %>">
      <%= bookmark.title %></li>
    </a>
    <form action="/bookmarks/<%= bookmark.id %>" method="post">
        <input type='hidden' name='_method' value='DELETE'/>
        <input type="submit" value="Delete" />
      </form>
    </li>
  <% end %>
</ul>